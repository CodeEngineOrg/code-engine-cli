{
  "name": "@code-engine/cli",
  "version": "0.0.0",
  "description": "build tool | code generator | static site builder",
  "keywords": [
    "build-tool",
    "asset-pipeline",
    "code-generator",
    "code-generation",
    "static-site-builder",
    "static-site-generator",
    "static",
    "site",
    "website",
    "blog",
    "docs",
    "documentation",
    "generator",
    "build",
    "make",
    "tool",
    "pipeline",
    "parallel"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://engine.codes",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeEngineOrg/code-engine-cli.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage lib",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "mocha && npm run lint",
    "coverage": "nyc node_modules/mocha/bin/mocha",
    "upgrade": "npm-check -u && npm audit fix",
    "bump": "bump --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm run build && npm test && npm run bump"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@types/chai": "^4.2.5",
    "@types/command-line-args": "^5.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.14",
    "chai": "^4.2.0",
    "chai-exec": "^1.1.1",
    "coveralls": "^3.0.8",
    "eslint": "^6.7.1",
    "eslint-config-modular": "^7.0.0",
    "mocha": "^6.2.2",
    "npm-check": "^5.9.0",
    "nyc": "^14.1.1",
    "shx": "^0.3.2",
    "source-map-support": "^0.5.16",
    "tslint": "^5.20.1",
    "tslint-modular": "^1.4.1",
    "typescript": "^3.7.2",
    "typescript-tslint-plugin": "^0.5.5",
    "version-bump-prompt": "^5.0.6"
  },
  "dependencies": {
    "@code-engine/lib": "0.0.6",
    "code-engine-destination-filesystem": "0.0.3",
    "code-engine-source-filesystem": "0.0.3",
    "command-line-args": "^5.1.1"
  }
}
