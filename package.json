{
  "name": "@code-engine/cli",
  "version": "0.0.12",
  "description": "build tool | code generator | static site builder",
  "keywords": [
    "build-tool",
    "asset-pipeline",
    "code-generator",
    "code-generation",
    "static-site-builder",
    "static-site-generator",
    "static",
    "site",
    "website",
    "blog",
    "docs",
    "documentation",
    "generator",
    "build",
    "make",
    "tool",
    "pipeline",
    "parallel"
  ],
  "author": {
    "name": "James Messinger",
    "url": "https://jamesmessinger.com"
  },
  "license": "MIT",
  "homepage": "https://engine.codes",
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeEngineOrg/code-engine-cli.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "shx rm -rf .nyc_output coverage lib",
    "lint": "npm run lint:typescript && npm run lint:javascript",
    "lint:typescript": "tslint -p tsconfig.json",
    "lint:javascript": "eslint test",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "mocha && npm run lint",
    "coverage": "nyc node_modules/mocha/bin/mocha",
    "upgrade": "npm-check -u && npm audit fix",
    "bump": "bump --tag --push --all",
    "release": "npm run upgrade && npm run clean && npm run build && npm test && npm run bump"
  },
  "engines": {
    "node": ">=12.12"
  },
  "engineStrict": true,
  "devDependencies": {
    "@code-engine/types": "^1.0.5",
    "@jsdevtools/eslint-config-modular": "^8.0.0",
    "@jsdevtools/host-environment": "2.0.2",
    "@jsdevtools/tslint-modular": "^2.0.2",
    "@jsdevtools/version-bump-prompt": "^6.0.2",
    "@types/chai": "^4.2.7",
    "@types/command-line-args": "^5.0.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.1.8",
    "@types/source-map-support": "^0.5.0",
    "@types/update-notifier": "^4.1.0",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "eslint": "^6.8.0",
    "mocha": "^7.0.0",
    "npm-check": "^5.9.0",
    "nyc": "^15.0.0",
    "shx": "^0.3.2",
    "sinon": "^9.0.0",
    "source-map-support": "^0.5.16",
    "tmp": "^0.1.0",
    "tslint": "^6.0.0",
    "typescript-tslint-plugin": "^0.5.5"
  },
  "dependencies": {
    "@code-engine/lib": "^1.0.0",
    "@code-engine/utils": "^1.0.0",
    "@code-engine/validate": "^1.0.1",
    "code-engine-destination-filesystem": "^1.0.0",
    "code-engine-source-filesystem": "^1.0.0",
    "command-line-args": "^5.1.1",
    "filesize": "^6.0.1",
    "find-up": "^4.1.0",
    "@jsdevtools/ono": "^7.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "update-notifier": "^4.0.0"
  }
}
